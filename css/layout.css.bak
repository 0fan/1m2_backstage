/* mod-news */
.mod-news{padding: 20px 40px 100px;background: #fff;border: 1px solid #e6e6e6;}
.mod-news  .mod-news-h{border-bottom: 1px solid #ededed;}
.mod-news  .mod-news-h .tit{margin-bottom: 10px;height: 50px;line-height: 50px;}
.mod-news  .mod-news-h .tit .txt{font-size: 20px;color: #1a1a1a;}
.mod-news .mod-table{box-shadow: none;margin: 0;padding: 0;}
.mod-news .mod-table td > a{color: #36c;display: inline-block;border: 1px solid transparent;}
.mod-news .mod-table td > a:hover{border-bottom-color:#36c;}
.mod-news .mod-table td.news-status{color: #f44336;}
.mod-news .mod-table td.news-status.ed{color: #999;}
/* end mod-news */

/* mod-tit */
.mod-tit{height: 40px;line-height: 40px;padding-left: 13px;position: relative;}
.mod-tit:after{content: "";display: block;width: 4px;height: 18px;background-color: #2dae36;top: 11px;left: 0;border-radius: 2px;position: absolute;margin-bottom: 20px;}
.mod-tit .txt{font-size: 16px;color: #1a1a1a;}
/* end mod-tit */
/* mod-panel */
.mod-panel{padding: 15px;border: 1px solid #ddd;background-color: #fff;position: relative;overflow-y: hidden;}
