.mod-select .mod-select-b .mod-select-list-content-wrap{overflow:hidden;margin-right: -10px;margin-bottom: -10px;}
.mod-select .mod-select-b .mod-select-list-content-item{float: left;}
.mod-select .mod-select-b .mod-select-list-content-item a{display: block;margin-right: 16px;margin-bottom: 10px;font-size: 14px;color: #333;}
.mod-select .mod-select-b .mod-select-list-content-item a:hover,.mod-select .mod-select-b .mod-select-list-content-item a.h{color: #2196f3;}
